{
  "name": "ring-client-example",
  "version": "1.0.0",
  "description": "Example use of the `ring-client-api` npm package",
  "main": "lib/index.js",
  "scripts": {
    "start": "npm run ts-example",
    "auth": "ring-auth-cli",
    "ts-example": "tsc && node --env-file=.env lib/example.js",
    "ring-to-open": "tsc && node --env-file=.env lib/ring-to-open.js",
    "test-intercom": "tsc && node --env-file=.env lib/test-intercom.js",
    "set-autounlock": "tsc && node --env-file=.env lib/set-autounlock-mode.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dgreif/ring-client-example.git"
  },
  "bugs": {
    "url": "https://github.com/dgreif/ring-client-example/issues"
  },
  "homepage": "https://github.com/dgreif/ring-client-example#readme",
  "dependencies": {
    "ring-client-api": "^13.2.1",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "typescript": "^5.8.2",
    "@types/node": "^20.10.0"
  }
}
